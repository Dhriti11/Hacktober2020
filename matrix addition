#include<stdio.h>
int main()
{
    int i,j,n;
    printf("enter the order of the matrices n");
    scanf("%d",&n);
    int A[n][n],B[n][n];
    printf("enter matrix A \n");
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            scanf("%d",&A[i][j]);
        }
        printf("\n");
    }
    printf("enter matrix B \n");
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            scanf("%d",&B[i][j]);
        }
        printf("\n");
    }
    printf("the sum of the matrices A and B is \n")
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("%d ",A[i][j]+B[i][j]);
        }
        printf("\n");
    }

}
